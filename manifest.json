{
  "manifest_version": 3,

  "name": "GitHub open with Github1s",
  "description": "",
  "version": "2.0",

  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://github.com/*"],
      "js": ["injectionScript.js"]
    }
  ],

  "action": {
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  }
}
